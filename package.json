{
  "name": "clinic-heap-profiler",
  "version": "1.0.0",
  "engines": {
    "node": ">= 10.12.0"
  },
  "description": "Programmable interface to Clinic.js Heap Profiler",
  "repository": "nearform/node-clinic-heap-profiler",
  "main": "src/index.js",
  "scripts": {
    "visualize-watch": "node debug/visualize-watch.js",
    "visualize-all": "node debug/visualize-all.js",
    "test": "npm run lint && tap",
    "test:file": "standard | snazzy && tap --no-cov",
    "ci-lint": "npm run lint",
    "ci-test-cov": "tap",
    "ci-test-no-cov": "tap --no-cov",
    "lint": "standard --fix | snazzy"
  },
  "tap": {
    "nyc-arg": [
      "--exclude=test/*.js",
      "--exclude=visualizer/d3.js",
      "--exclude=visualizer/data-tree.js",
      "--exclude=visualizer/no-data-node.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nearform/clinic-common": "^4.0.0",
    "@nearform/heap-profiler": "^0.5.0",
    "copy-to-clipboard": "^3.0.8",
    "d3-array": "^2.0.2",
    "d3-fg": "^6.13.1",
    "d3-selection": "^1.3.2",
    "fs-extra": "^9.0.1",
    "lodash.debounce": "^4.0.8",
    "on-net-listen": "1.1.2",
    "pump": "^3.0.0",
    "querystringify": "^2.1.0",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "chokidar": "^3.4.2",
    "fastify": "^3.5.1",
    "snazzy": "^8.0.0",
    "standard": "^14.3.1"
  }
}
